<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.demo">

    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="MissingClass">
        <activity android:name=".four.DynamicFragmentActivity"></activity>
        <activity android:name=".four.SimpleFragmentActivity" />
        <activity android:name=".six.LitePalActivity" />
        <activity android:name=".six.SQLiteActivity" />
        <activity android:name=".six.SharedPreferenceActivity" />
        <activity
            android:name=".six.FilePersistenceActivity"
            android:label="File persistence" />
        <activity android:name=".FourActivity" />
        <activity android:name=".SixActivity" />
        <activity
            android:name=".three.ChatActivity"
            android:label="chat" />
        <activity android:name=".three.RecyclerViewActivity" />
        <activity android:name=".three.ListviewActivity" />
        <activity android:name=".three.PercentFrameLayoutActivity" />
        <activity android:name=".three.FrameLayoutActivity" />
        <activity
            android:name=".three.RelativeLayoutActivity"
            android:label="Relative" />
        <activity
            android:name=".three.LinearLayoutActivity"
            android:label="Linear" />
        <activity
            android:name=".three.UIWidgetActivity"
            android:label="UIWidget" />
        <activity
            android:name=".DialogActivity"
            android:theme="@style/AppDialog" />
        <activity
            android:name=".NormalActivity"
            android:label="normal" />
        <activity
            android:name=".ChapterThreeActivity"
            android:label="第三个页面" />
        <activity
            android:name=".SecondActivity"
            android:label="第二个页面">
            <intent-filter>
                <action android:name="com.example.demo.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.example.demo.MY_CATEGORY" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FirstActivity"
            android:label="第一个页面" />
        <activity
            android:name=".MainActivity"
            android:label="首页">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>